<!DOCTYPE html>
<html>
<head>
 {% block head %}
    <meta charset="UTF-8">
    <title>{% block title %}Home{% endblock %}</title>
    <link rel="icon" type="image/png" href="{{ asset('icons/school.png' ) }}">
    <meta name="viewport" content="width=device-width, initial-scale=1">
     <link href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i" rel="stylesheet">

     {#<link href="{{ asset('assets/css/fullcalendar.css') }}" rel="stylesheet" />
     <link href="{{ asset('assets/css/fullcalendar.print.css') }}" rel="stylesheet" media="print" />#}
     <script src="{{ asset('assets/js/jquery-1.10.2.js') }}" type="text/javascript"></script>

     {#<script src="{{ asset('assets/js/fullcalendar.js') }}" type="text/javascript"></script>
     <script src="{{ asset('assets/js/calendar.js') }}" type="text/javascript"></script>#}
    {# <link rel="stylesheet" href="{{ asset('vendor/bootstrap/css/bootstrap.min.css') }}">#}
    {#<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css">#}
    {#<link href="/bootstrap4-glyphicons/css/bootstrap-glyphicons.min.css" rel="stylesheet" type="text/css" />#}
     {% block javascripts %}

         <!-- Renders app.js & a webpack runtime.js file
             <script src="/build/runtime.js"></script>
             <script src="/build/app.js"></script> -->
         {{ encore_entry_script_tags('app') }}
     {% endblock %}

    {% block stylesheets %}
        {# 'app' must match the first argument to addEntry() in webpack.config.js #}
        {{ encore_entry_link_tags('css/app') }}
        {# 'app' must match the first argument to addEntry() in webpack.config.js #}
        {{ encore_entry_link_tags('css/app') }}

        <!-- Renders a link tag (if your module requires any CSS)
             <link rel="stylesheet" href="/build/app.scss"> -->
    {% endblock %}
    {% endblock %}
</head>
<body>

<div class="main-content">
   {# // loading ...#}
    <div class="loading-spinner d-flex justify-content-center">
        <div class="spinner-grow text-info" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>
    {#// nav bar#}
    <nav class="navbar navbar-light bg-light">
        <a class="navbar-brand" href="#">
            <img src="{{ asset('icons/school.png' ) }}" width="30" height="30" alt="">
        </a>
        <div>

            <a href="{{ path('etudiant_all') }}" class="btn btn-default navbar-btn"><i class="fa fa-user-circle" aria-hidden="true"></i> Etudiants</a>
            <a href="{{ path('groupe_all') }}" class="btn btn-default navbar-btn"><i class="fa fa-users" aria-hidden="true"></i> Groupes</a>
            <a href="{{ path('professeur_all') }}" class="btn btn-default navbar-btn"><i class="fa fa-chalkboard-teacher" aria-hidden="true"></i> Professeurs</a>
            <a href="{{ path('module_all') }}" class="btn btn-default navbar-btn active"> <i class="fa fa-book" aria-hidden="true"></i> Module</a>
            <a href="{{ path('cours_all') }}" class="btn btn-default navbar-btn"><i class="fa fa-certificate" aria-hidden="true"></i> Cours</a>
            <a href="{{ path('emploi_du_temps') }}" class="btn btn-default navbar-btn"><i class="far fa-calendar-alt" aria-hidden="true"></i>  Emploi du Temps</a>
            <a href="{{ path('exam_all') }}" class="btn btn-default navbar-btn"><i class="fa fa-file" aria-hidden="true"></i> Exam</a>
            <a href="{{ path('note_all') }}" class="btn btn-default navbar-btn"><i class="fa fa-sticky-note" aria-hidden="true"></i> Note</a>
            <a href="{{ path('salle_all') }}" class="btn btn-default navbar-btn">Salles</a>
            <a href="" class="btn btn-default navbar-btn">logout</a>

            <a href="" class="btn btn-default navbar-btn">Register</a>
            <a href="" class="btn btn-default navbar-btn">login</a>

        </div>
        <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav navbar-right">
                <li>

                </li>
            </ul>
        </div>
    </nav>
    {#//container#}
    <div class="container">
        {% block body %}{% endblock %}
    </div>
</div>

</body>
</html>